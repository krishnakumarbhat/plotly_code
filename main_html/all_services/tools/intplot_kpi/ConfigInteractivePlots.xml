<?xml version="1.0" encoding="UTF-8"?>
<!-- Delphi SIL component configuration -->
<Interactiveplot_Extracter_Configuration>

  <Interactiveplot_Extracter_Configuration_Config_XML_Version>2.0</Interactiveplot_Extracter_Configuration_Config_XML_Version>

  <!-- SOURCE SELECTION
       Options:
         MUDP    : Input and output HDF generated from MUDP Extractor (UDP Data)
         BORDNET : Input and output HDF generated from BORDNET (CAN Data)
  -->

  <!-- HDF_SOURCE_SELECTION
  i you are giveing MF$4 as input in json select it 
       MUDP    : Input/output HDF from MUDP Extractor (UDP Data) â€” requires RDD data (e.g., rdd_index, rdd_rindex)
       BORDNET : Input/output HDF from BORDNET (CAN Data)
  -->
  <HDF_SOURCE_SELECTION>MUDP</HDF_SOURCE_SELECTION>

  <!-- HDF_FILE
       HDF_WITH_ALLSENSOR : A single HDF5 file containing data from all sensors.
       HDF_PER_SENSOR     : Separate HDF5 files per sensor (RR, RL, FL, FR).
       HDF_PAIRED_FILES   : Separate HDF files per stream.
       DC_HDF_FORMAT      : Data Collector HDF format.
       HIL_HDF_FORMAT     : HIL HDF format.
       CAN_HDF_FORMAT     : CAN HDF format.
  -->
  <HDF_FILE>HDF_WITH_ALLSENSOR</HDF_FILE>

  <!-- PLOT_MODE
       PLOT_OVERLAY
         0 : Disabled (default)
         1 : Generate interactive HTML reports with overlay plots between file1 and file2
       VIDEO_OVERLAY
         0 : Disabled (default)
         1 : Generate video reports between file1 and file2 tracker data
       D3_VISUALIZATION
         0 : Disabled (default)
         1 : Generate 3D visualization web UI for detections/tracks with video overlay
       KPI
         0 : Disabled (default)
         1 : Generate interactive KPI reports between file1 and file2
       ALLUR_REPORT
         0 : Disabled (default)
         1 : Generate high-level test reports between file1 and file2
  -->
  <PLOT_MODE>
    <PLOT_OVERLAY>1</PLOT_OVERLAY>
    <KPI>0</KPI>
    <RAF_DATA_TEXT/>
    <VIDEO_OVERLAY>0</VIDEO_OVERLAY>
    <D3_VISUALIZATION>0</D3_VISUALIZATION>
    <ALLUR_REPORT>0</ALLUR_REPORT>
  </PLOT_MODE>

</Interactiveplot_Extracter_Configuration>